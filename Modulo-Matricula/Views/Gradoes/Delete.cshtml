@model Modulo_Matricula.Grado

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .myBtn {
        border: 1px solid #3f66fb;
        border-radius: 5px;
    }
</style>
<h2>Eliminar asignación de grado.</h2>

<h3>¿Deseas eliminar esta asignación de grado?</h3>
<div>
    @*<h4>Grado</h4>*@
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Grupo)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Grupo)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.CodigoNacionalDeEstudiante)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CodigoNacionalDeEstudiante)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.EstadoDelEstudiante)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.EstadoDelEstudiante)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.CentroDeProcedencia)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CentroDeProcedencia)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Alumnos.Nombre)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Alumnos.Nombre)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <div class="btn btn-default myBtn">
                @Html.ActionLink("Regresar", "Index")
            </div>
            <input type="submit" value="Eliminar" class="btn btn-danger" />
        </div>
    }
</div>
